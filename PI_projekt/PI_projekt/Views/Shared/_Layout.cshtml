<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - PI_projekt</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <!--<<link rel="stylesheet" href="~/css/site.css" />-->
    <link href="~/css/LayoutPom.css" rel="stylesheet" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light border-bottom box-shadow mb-3 navLayout">
            <div class="container navLayout">
                <a class="navbar-brand navA" asp-area="" asp-controller="Home" asp-action="Profil">PI_projekt</a>
                <button class="navbar-toggler btn-primary" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between navLayout">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark navA" asp-area="" asp-controller="Home" asp-action="Profil">Profil</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark navA" asp-area="" asp-controller="KorisnikBiljke" asp-action="KorisnikBiljkeIndex">Biljke</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark navA" asp-area="" asp-controller="Prodaja" asp-action="NarudzbeIndex">Narudžbe</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark navA" asp-area="" asp-controller="BezNarudzbi" asp-action="ProdajaIndex">Prodaja_bez_narudžbi</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark navA" asp-area="" asp-controller="Troskovi" asp-action="TroskoviIndex">Troškovi</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark navA" asp-area="" asp-controller="Alat" asp-action="AlatIndex">Alat</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark navA" asp-area="" asp-controller="Lokacija" asp-action="LokacijaIndex">Parcele</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark navA" asp-area="" asp-controller="Home" asp-action="Odjava">Odjava</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - PI_projekt <!-- <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>-->
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
